{
    "name": "konekt/menu",
    "description": "Menu Library for Laravel v11+",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel"
    ],
    "support": {
        "issues": "https://github.com/artkonekt/menu"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        },
        {
            "name": "Lavary",
            "email": "mrl.8081@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Menu\\": "src/"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit" : "^10.0|^11.0",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Menu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Konekt\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Konekt\\Menu\\Facades\\Menu"
            }
        }
    }
}
